
{
    "manifest_version" : 2,
    "name" : "Password Generator",
    "version" : "1.0",
    "description" : "generate a new password",
    "icons" : {
        "128" : "logo.png",
        "48" : "logo.png",
        "16" : "logo.png"
    },
    "browser_action" : {
        "default_icon" : "logo.png",
        "default_popup" : "index.html"
    },
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
